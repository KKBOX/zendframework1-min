{
    "name": "zend/zendframework",
    "description": "Zend Framework 1 fork with minimum require_once and include_path",
    "type": "library",
    "keywords": [
        "zf1",
        "zend",
        "zendframework"
    ],
    "homepage": "https://github.com/cdsinternetagentur/zf1-composer",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.2.11"
    },
    "include-path": ["library/"],
    "autoload": {
        "psr-0": {"Zend": "library/"}
    },
    "replace": {
        "zendframework/zendframework1": "1.12.*"
    },
    "conflict": {
        "zendframework/zendframework1": "<1.12"
    }
}
